<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.hit_src.ui.OverviewActivity">

    <LinearLayout
        android:id="@+id/Mainui_base_LinearLayout"
        android:layout_width="1278dp"
        android:layout_height="661dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/Mainui_content_LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="575dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/Mainui_serial_LinearLayout"
                android:layout_width="334dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/Mainui_serialarea_title_TextView"
                    android:layout_width="match_parent"
                    android:layout_height="91dp"
                    android:gravity="bottom|center_horizontal"
                    android:text="@string/Mainui_serialarea_Title"
                    android:textSize="30sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Mainui_overview_LinearLayout"
                android:layout_width="623dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/Mainui_overviewarea_title_textview"
                    android:layout_width="match_parent"
                    android:layout_height="63dp"
                    android:gravity="bottom|center_horizontal"
                    android:text="@string/Mainui_overviewarea_Title"
                    android:textSize="40sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Mainui_internet_LinearLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/Mainui_internetarea_title_TextView"
                    android:layout_width="match_parent"
                    android:layout_height="91dp"
                    android:gravity="bottom|center_horizontal"
                    android:text="@string/Mainui_internetarea_Title"
                    android:textSize="30sp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/Mainui_guide_LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/Mainui_Overview_Button"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/OverviewButtonText"
                android:textSize="25sp" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
